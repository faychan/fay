
@model fay.Models.ViewModel.outbondDataView
<div>
    <table class="table table-striped table-condensed table-hover">
        <thead>
            <tr>
                @{
                    fay.Models.EntityManager.UserManager um = new fay.Models.EntityManager.UserManager();
                    
                }
                <th>outbond ID</th>
                <th>Price</th>
                <th>Description</th>
                <th>Actions</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var i in Model.outbondProfile)
            {
                <tr>
                   
                        <td>@Html.DisplayFor(m => i.id_outbond)/td>
                    <td> @Html.DisplayFor(m => i.harga)</td>
                        <td> @Html.DisplayFor(m => i.keterangan)</td>
                        <td class="btn-group-sm"><button class="btn btn-info outbondEdit">Edit</button> <button class="btn btn-danger outbondDelete">Delete</button></td>
                   
                </tr>
            }
        </tbody>
    </table>
</div>
<!-- DIV FOR ANYTHING RELATED TO EDIT AND DELETE -->

    @Html.Action("AdminoutbondPartial", "outbond")
